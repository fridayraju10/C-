#include<iostream>
using namespace std;
int main()
{
    for(int j=1;j<=20;j++)
    {
        for(int i=1;i<j;i++)
        {
            for(int k=1;k<i;k++)
            {
                if(k*k + i*i == j*j)
                {
                    cout<<k<<"  "<<i<<" "<<j<<endl;
                }
            }
        }
}
return 0;
}
